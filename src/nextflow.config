//Enable trace by default
trace.enabled = true

profiles {
  standard {
    process.executor = 'local'
    queueSize = 10
    pollInterval = '1sec'
  }
  cluster {
    process.executor = 'sge'
    process.queue = 'long'
    process.memory = '10GB'
    queueSize = 100
    pollInterval = '30sec'
  }
}
